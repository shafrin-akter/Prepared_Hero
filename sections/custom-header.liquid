{% liquid
  assign menus = section.settings.menu.links
%}
<div class="headerSection">
  <div class="container">
    <div class="logo"></div>
    <nav>
      <ul>
        {% for menu in menus %}
          <li>
            <strong>URL</strong>{{ menu.url }}
            <strong>title</strong>{{ menu.title }}
            <strong>levels</strong>{{ menu.levels }}
            <strong>handle</strong>{{ menu.handle }}
          </li>
        {% endfor %}
      </ul>
    </nav>
    <div class="cart"></div>
  </div>
</div>
{% schema %}
  {
    "name": "Custom Header",
    "settings": [
      {
        "type": "link_list",
        "id": "menu",
        "label": "Menu"
      }
    ]
  }
{% endschema %}
{% liquid
  assign menus = section.settings.menu
%}
<div class="headerSection">
  <div class="container">
    <div class="logo"></div>
    <nav>
      <ul>
        {% for menu in menus %}
          
        {% endfor %}
      </ul>
    </nav>
    <div class="cart"></div>
  </div>
</div>
{% schema %}
  {
    "name": "Custom Header",
    "settings": [
      {
        "type": "link_list",
        "id": "menu",
        "label": "Menu"
      }
    ]
  }
{% endschema %}